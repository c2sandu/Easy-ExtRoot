config 'global'
	option	anon_swap	'1'
	option	anon_mount	'0'
	option	auto_swap	'1'
	option	auto_mount	'1'
	option	delay_root	'10'
	option	check_fs	'0'

config 'mount'
	option	target	'/mnt/sda2'
	option	enabled	'1'

config 'swap'
	option device '/dev/sda1'
	option	enabled	'1'

config mount
	option target /
	option device /dev/sda2
	option fstype ext4
	option options rw,sync
	option enabled 1
	option enabled_fsck 0

